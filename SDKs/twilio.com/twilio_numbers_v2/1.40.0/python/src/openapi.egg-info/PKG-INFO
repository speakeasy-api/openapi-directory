from sdk.models import operations, shared

s = sdk.SDK()


req = operations.CreateBundleCreateBundleRequest(
    email="Larue_Rau85@yahoo.com",
    end_user_type="business",
    friendly_name="illum",
    iso_country="vel",
    number_type="error",
    regulation_sid="deserunt",
    status_callback="http://innocent-effect.org",
)
    
